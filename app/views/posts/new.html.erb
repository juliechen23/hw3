<h1>New Post</h1>

<%= form_with :model => @post do |form| %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </p>
  <p>
    <%= form.label "body" %>
    <%= form.text_field "body" %>
  </p>
  <p>
    <%= form.label "date" %>
    <%= form.date_field "date" %>
  </p>
    <%= form.hidden_field "place_id" %>
  <%= form.submit %>
<% end %>